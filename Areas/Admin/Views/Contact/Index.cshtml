@model Contact

<table class="table table-dark">
    <thead>
        <tr>
            <th> Adress </th>
            <th> Phone1 </th>
            <th> Phone1 </th>
            <th> Email </th>
            <th> WebSite </th>
            <th> Edit </th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td> @Model.Address </td>
            <td> @Model.Phone1 </td>
            <td> @Model.Phone1 </td>
            <td> @Model.Email </td>
            <td> @Model.WebSite </td>
            <td> <a asp-action="EditContact" class="btn btn-primary"> Edit </a> </td>
        </tr>
    </tbody>
</table>
