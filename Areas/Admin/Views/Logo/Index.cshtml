@model Logo

<table class="table table-dark">
    <thead>
        <tr>
            <th> Id </th>
            <th> Photo </th>
            <th> Edit </th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>@Model.Id</td>
            <td><img src="~/img/@Model.Photo"></td>
            <td> <a asp-action="EditLogo" class="btn btn-primary"> Edit </a> </td>
        </tr>
    </tbody>
</table>