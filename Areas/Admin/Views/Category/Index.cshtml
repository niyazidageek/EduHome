@model CategoryVM

<a asp-controller="Category" asp-action="CreateCategory" class="btn btn-success">Create</a>
<table class="table table-dark">
    <thead>
        <tr>         
            <th> Category name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var category in Model.Names)
        {
            <tr>
                <td> @category </td>               
            </tr>
        }
    </tbody>
</table>
